<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FormSubmited">

    <Spinner
        android:id="@+id/spinnerGender"
        android:layout_width="268dp"
        android:layout_height="38dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="209dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinnerCinema"
        android:layout_width="268dp"
        android:layout_height="38dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="344dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="349dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="130dp"
        android:layout_height="30dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="203dp"
        android:layout_marginBottom="27dp"
        android:text="Género:"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/spinnerGender"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="248dp"
        android:layout_height="32dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="85dp"
        android:text="Género cinematográfico:"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerGender" />

    <Button
        android:id="@+id/buttonNextActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="219dp"
        android:text="Next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerCinema" />
</androidx.constraintlayout.widget.ConstraintLayout>